{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore.Relational": "Error",
        "Microsoft": "Error",
        "System.ServiceModel.Http": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=tcp:sqlserver-vrt-pro.database.windows.net,1433;Initial Catalog=sql-db-vrt-pro;Persist Security Info=False;User ID=admin_su;Password=P@ssword;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;",
          "schemaName": "Vivec",
          "tableName": "VivecLogs",
          "autoCreateSqlTable": true,
          "columnOptionsSection": {
            "removeStandardColumns": [ "MessageTemplate", "Properties" ],
            "customColumns": [
              {
                "ColumnName": "Process",
                "DataType": "nvarchar"
              }
            ]
          }
        },
        "restrictedToMinimumLevel": "Warning"
      }
    ]
  },
  "DetailedErrors": true,
